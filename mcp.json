{
  "name": "trello",
  "displayName": "Trello",
  "description": "Manage Trello boards, lists, cards, and organizations directly from Kiro",
  "icon": "logo.png",
  "mcpServers": {
    "trello": {
      "command": "uvx",
      "args": ["--from", "git+https://github.com/cargom98/gm-trello-mcp.git", "trello_mcp_server"],
      "env": {
        "TRELLO_API_KEY": "YOUR_TRELLO_API_KEY"
      }
    }
  }
}
